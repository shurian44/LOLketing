<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomEditTextView">
        <attr name="textColor" format="reference"/>
    </declare-styleable>
    <declare-styleable name="CustomEventCardView">
        <attr name="eventImage" format="reference"/>
        <attr name="eventTitle" format="string"/>
        <attr name="eventPeriod" format="string"/>
    </declare-styleable>
    <declare-styleable name="CustomSeatCheckBox">
        <attr name="seatChecked" format="boolean"/>
        <attr name="seatEnable" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="CustomSeatRowLayout">
        <attr name="seatSize" format="integer"/>
        <attr name="startStr" format="string"/>
    </declare-styleable>
</resources>