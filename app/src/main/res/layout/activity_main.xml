<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/main_logo"
                android:layout_width="80dp"
                android:layout_height="30dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="6dp"
                android:src="@drawable/logo_small"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"/>

            <TextView
                android:id="@+id/btn_logout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="10dp"
                android:text="로그아웃"
                android:onClick="logout"
                android:textStyle="bold"
                android:textColor="@color/colorPrimaryDark"
                app:layout_constraintTop_toTopOf="@id/main_logo"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="@id/main_logo"/>

            <ImageView
                android:id="@+id/img_ad"
                android:layout_width="0dp"
                android:layout_height="250dp"
                android:layout_marginTop="6dp"
                android:src="@drawable/img_test"
                android:scaleType="fitXY"
                app:layout_constraintTop_toBottomOf="@id/main_logo"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>

            <ImageView
                android:id="@+id/btn_notice"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_marginTop="10dp"
                android:src="@drawable/icon_notice"
                android:onClick="moveActivity"
                app:layout_constraintTop_toBottomOf="@id/img_ad"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toLeftOf="@id/btn_event"/>

            <ImageView
                android:id="@+id/btn_event"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/icon_event"
                android:onClick="moveActivity"
                app:layout_constraintLeft_toRightOf="@id/btn_notice"
                app:layout_constraintTop_toTopOf="@id/btn_notice"
                app:layout_constraintRight_toLeftOf="@id/btn_myPage"/>

            <ImageView
                android:id="@+id/btn_myPage"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/icon_mypage"
                android:onClick="moveActivity"
                app:layout_constraintLeft_toRightOf="@id/btn_event"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@id/btn_notice"/>

            <TextView
                android:id="@+id/txt_notice"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="공지사항"
                android:textColor="#000"
                android:textAlignment="center"
                android:layout_marginTop="6dp"
                app:layout_constraintTop_toBottomOf="@id/btn_notice"
                app:layout_constraintLeft_toLeftOf="@id/btn_notice"
                app:layout_constraintRight_toRightOf="@id/btn_notice"/>

            <TextView
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="이벤트"
                android:textColor="#000"
                android:textAlignment="center"
                android:layout_marginTop="6dp"
                app:layout_constraintTop_toBottomOf="@id/btn_event"
                app:layout_constraintLeft_toLeftOf="@id/btn_event"
                app:layout_constraintRight_toRightOf="@id/btn_event"/>

            <TextView
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="내 정보"
                android:textColor="#000"
                android:textAlignment="center"
                android:layout_marginTop="6dp"
                app:layout_constraintTop_toBottomOf="@id/btn_myPage"
                app:layout_constraintLeft_toLeftOf="@id/btn_myPage"
                app:layout_constraintRight_toRightOf="@id/btn_myPage"/>

            <ImageView
                android:id="@+id/btn_info"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_marginTop="10dp"
                android:src="@drawable/icon_info_small"
                android:onClick="moveActivity"
                app:layout_constraintTop_toBottomOf="@id/txt_notice"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toLeftOf="@id/btn_event"/>

            <ImageView
                android:id="@+id/btn_reserve"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/icon_reserve"
                android:onClick="moveActivity"
                app:layout_constraintTop_toTopOf="@id/btn_info"
                app:layout_constraintLeft_toRightOf="@id/btn_event"
                app:layout_constraintRight_toLeftOf="@id/btn_event"/>

            <ImageView
                android:id="@+id/btn_shop"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/icon_shop"
                android:onClick="moveActivity"
                app:layout_constraintTop_toTopOf="@id/btn_info"
                app:layout_constraintLeft_toRightOf="@id/btn_reserve"
                app:layout_constraintRight_toRightOf="parent"/>

            <TextView
                android:id="@+id/txt_info"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="리그정보"
                android:textColor="#000"
                android:textAlignment="center"
                android:layout_marginTop="6dp"
                app:layout_constraintTop_toBottomOf="@id/btn_info"
                app:layout_constraintLeft_toLeftOf="@id/btn_info"
                app:layout_constraintRight_toRightOf="@id/btn_info"/>

            <TextView
                android:id="@+id/txt_reserve"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="티켓예매"
                android:textColor="#000"
                android:textAlignment="center"
                android:layout_marginTop="6dp"
                app:layout_constraintTop_toBottomOf="@id/btn_reserve"
                app:layout_constraintLeft_toLeftOf="@id/btn_reserve"
                app:layout_constraintRight_toRightOf="@id/btn_reserve"/>

            <TextView
                android:id="@+id/txt_shop"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="샵"
                android:textColor="#000"
                android:textAlignment="center"
                android:layout_marginTop="6dp"
                app:layout_constraintTop_toBottomOf="@id/btn_shop"
                app:layout_constraintLeft_toLeftOf="@id/btn_shop"
                app:layout_constraintRight_toRightOf="@id/btn_shop"/>



            <ImageView
                android:id="@+id/btn_guid"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_marginTop="10dp"
                android:src="@drawable/icon_lol"
                android:onClick="moveActivity"
                app:layout_constraintTop_toBottomOf="@id/txt_info"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toLeftOf="@id/btn_event"/>

            <ImageView
                android:id="@+id/btn_news"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/icon_news"
                android:onClick="moveActivity"
                app:layout_constraintTop_toTopOf="@id/btn_guid"
                app:layout_constraintLeft_toRightOf="@id/btn_chatting"
                app:layout_constraintRight_toLeftOf="@id/btn_guid"/>

            <ImageView
                android:id="@+id/btn_chatting"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/icon_chatting"
                android:onClick="moveActivity"
                app:layout_constraintTop_toTopOf="@id/btn_guid"
                app:layout_constraintLeft_toRightOf="@id/btn_news"
                app:layout_constraintRight_toRightOf="parent"/>

            <TextView
                android:id="@+id/txt_guid"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="롤알못"
                android:textColor="#000"
                android:textAlignment="center"
                android:layout_marginTop="6dp"
                app:layout_constraintTop_toBottomOf="@id/btn_guid"
                app:layout_constraintLeft_toLeftOf="@id/btn_guid"
                app:layout_constraintRight_toRightOf="@id/btn_guid"/>

            <TextView
                android:id="@+id/txt_news"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="뉴스"
                android:textColor="#000"
                android:textAlignment="center"
                android:layout_marginTop="6dp"
                app:layout_constraintTop_toBottomOf="@id/btn_news"
                app:layout_constraintLeft_toLeftOf="@id/btn_news"
                app:layout_constraintRight_toRightOf="@id/btn_news"/>

            <TextView
                android:id="@+id/txt_chatting"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="응원채팅"
                android:textColor="#000"
                android:textAlignment="center"
                android:layout_marginTop="6dp"
                app:layout_constraintTop_toBottomOf="@id/btn_chatting"
                app:layout_constraintLeft_toLeftOf="@id/btn_chatting"
                app:layout_constraintRight_toRightOf="@id/btn_chatting"/>

            <TextView
                android:id="@+id/textViewAddress"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:text="구단 게시판"
                android:textColor="@color/colorPrimaryDark"
                android:textStyle="bold"
                app:layout_constraintTop_toBottomOf="@id/txt_guid"
                app:layout_constraintRight_toRightOf="@id/txt_guid"/>


            <ImageView
                android:id="@+id/icon_t1"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/logo_t1"
                android:onClick="moveActivity"
                android:layout_marginTop="20dp"
                app:layout_constraintTop_toBottomOf="@id/textViewAddress"
                app:layout_constraintLeft_toLeftOf="@id/txt_guid"/>

            <ImageView
                android:id="@+id/icon_griffin"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/icon_griffin"
                android:onClick="moveActivity"
                app:layout_constraintTop_toTopOf="@id/icon_t1"
                app:layout_constraintLeft_toLeftOf="@id/btn_news"/>

            <ImageView
                android:id="@+id/icon_damwon"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/icon_damwon"
                android:onClick="moveActivity"
                app:layout_constraintTop_toTopOf="@id/icon_t1"
                app:layout_constraintLeft_toLeftOf="@id/btn_chatting"/>

            <ImageView
                android:id="@+id/icon_sendbox"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/icon_sandbox"
                android:onClick="moveActivity"
                android:layout_marginTop="23dp"
                app:layout_constraintTop_toBottomOf="@id/icon_t1"
                app:layout_constraintLeft_toLeftOf="@id/icon_t1"/>

            <ImageView
                android:id="@+id/icon_freecs"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/icon_afreeca"
                android:onClick="moveActivity"
                app:layout_constraintTop_toTopOf="@id/icon_sendbox"
                app:layout_constraintLeft_toLeftOf="@id/icon_griffin"/>

            <ImageView
                android:id="@+id/icon_geng"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/icon_geng"
                android:onClick="moveActivity"
                app:layout_constraintTop_toTopOf="@id/icon_sendbox"
                app:layout_constraintLeft_toLeftOf="@id/icon_damwon"/>

            <ImageView
                android:id="@+id/icon_dragonx"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/icon_dragonx"
                android:layout_marginTop="23dp"
                android:onClick="moveActivity"
                app:layout_constraintTop_toBottomOf="@id/icon_sendbox"
                app:layout_constraintLeft_toLeftOf="@id/icon_sendbox"/>

            <ImageView
                android:id="@+id/icon_rolster"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/icon_rolster"
                android:onClick="moveActivity"
                app:layout_constraintTop_toTopOf="@id/icon_dragonx"
                app:layout_constraintLeft_toLeftOf="@id/icon_freecs"/>

            <ImageView
                android:id="@+id/icon_prince"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/icon_apk_prince"
                android:onClick="moveActivity"
                app:layout_constraintTop_toTopOf="@id/icon_dragonx"
                app:layout_constraintLeft_toLeftOf="@id/icon_geng"/>

            <ImageView
                android:id="@+id/icon_hanhwa"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/icon_hanwha"
                android:layout_marginTop="23dp"
                android:layout_marginBottom="20dp"
                android:onClick="moveActivity"
                app:layout_constraintTop_toBottomOf="@id/icon_dragonx"
                app:layout_constraintLeft_toLeftOf="@id/icon_rolster"
                app:layout_constraintBottom_toBottomOf="parent"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>